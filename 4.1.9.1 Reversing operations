#include <iostream>
#include <vector>
#include <algorithm>
#include <string>

using namespace std;

struct Compare
{

	bool operator()(const int &f, const int &s)
	{
		return f==s;
	}
};

int main()
{
	vector<int> values = { 1, 5, 6, 7, 8 };
	int a[5];
	for (int i = 0; i < 5; i++) {
		cin >> a[i];
	}
	vector<int> v(a, a + sizeof a / sizeof a[0]);

	reverse(values.begin(), values.end());

	pair<vector<int>::iterator, vector<int>::iterator> p;

	p = mismatch(values.begin(), values.end(), v.begin(), Compare());

	if (p.first == values.end())
		cout << "Palindrome." << endl;
	else cout << "Not palindrome." << endl;

	system("pause");
	return 0;
}
