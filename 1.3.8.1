#include <iostream>
#include <vector>

using namespace std;

int main()
{
	int b;
	vector<int> v1;
	vector<int> v2;
	cin >> b;
	if (b%2!=0)
	{
		b++;
	}
	for (int i = 1; i <= b; i++)
	{
		if (i % 2 != 0)
			v1.push_back(i);
		else
			v2.push_back(i);
	}
	for (auto it1 = v1.begin(); it1 != v1.end(); ++it1)
	{
		for (auto it2 = v2.begin(); it2 != v2.end(); ++it2)
		{
			cout << *it1 + *it2 << " ";
		}
	}
	for (auto j = v1.rbegin(); j != v1.rend(); j++)
	{
		for (auto i = v2.rbegin(); i != v2.rend(); i++)
		{
			cout << *j + *i << " ";
		}

	}
	cout << endl;

	system("pause");
	return 0;
}
