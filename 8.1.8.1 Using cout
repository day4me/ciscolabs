#include "stdafx.h"
#include <vector>
#include <iostream>
#include <string>

using namespace std;

int main()
{
	string s = "He held out his hand for the dog to sniff, but Nikolai nuzzled his nose into it instead";
	int start_value = 0, stop_value = 0;
	cin >> start_value;
	cin >> stop_value;
	//your code
	if (start_value < stop_value && stop_value< s.size() && start_value>=0)
	{
		for (int i = start_value; i < stop_value; i++)
		{
			if (s[i] != ' ') 
			{
				cout << s[i] << " ";
			}
		}
	}
	std::cout << endl;

	system("pause");
	return 0;
}
