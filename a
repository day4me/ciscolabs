#include <iostream>
#include <string>
#include <vector>
#include <algorithm>

using namespace std;

void print(const int & value)
{
	cout << value << " ";
}

struct Generate
{
	int s;
	int n;
	Generate(int n1) : s(1),n(n1){}
	int operator()()
	{
		do {
			return s++;
		} while (s!= n);
	}
};

int main()
{
	vector<int> results;
	int count;
	cin >> count;
	generate(results.begin(), results.end(), Generate(count));

	return 0;
}
