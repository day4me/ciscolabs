#include <iostream>
#include <vector>

using namespace std;


int main()
{
	vector<int> v1{ 1, 2, 3 };
	vector<int> v2{ 5, 2, 6, 4 };
	for (auto it1 = v1.crbegin(); it1 != v1.crend(); it1++)
	{
		for (auto it2 = v2.cbegin(); it2 != v2.cend(); it2++)
		{	
			cout << *it1 + *it2 << " ";
		}
		cout << endl;
	}
	system("pause");
	return 0;
}
