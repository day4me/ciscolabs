#include <iostream>
#include <string>
#include <vector>
#include <algorithm>

using namespace std;

void print(const int & value)
{
	cout << value << " ";
}

struct Generate
{
	int s;
	int n;
	int i;
	Generate(int n1) : s(0),n(n1),i(1) {}
	int operator()()
	{
			s += i;
			i++;
			return s;
	}
};

int main()
{
	vector<int> resultsSmall(6);
	int count;
	cin >> count;
	vector<int> results(count);

	generate_n(resultsSmall.begin(), 6, Generate(6));
	generate_n(results.begin(), count, Generate(count));

	for_each(resultsSmall.begin(), resultsSmall.end(), print);
	for_each(results.begin(), results.end(), print);

	system("pause");
	return 0;
}
